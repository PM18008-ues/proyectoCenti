\n          <v-row>\n            <v-col lg=\"12\">\n              <v-card>\n                <v-card-text>\n                  <v-row>\n                    <v-col lg=\"6\">\n                      <span>Vendedor: {{ anuncio.vendedor }}</span>\n                    </v-col>\n                    <v-col lg=\"6\">\n                      <span>Telefono: {{ anuncio.telefono }}</span>\n                    </v-col>\n                  </v-row>\n                </v-card-text>\n              </v-card>\n            </v-col>\n          </v-row>\n        </v-col>\n      </v-row>\n      <v-row>\n        <v-col lg=\"12\">\n          <v-tabs>\n            <v-tab>Description</v-tab>\n            <v-tab>Especificaciones</v-tab>\n            <v-tab-item>\n              <p class=\"pt-10 subtitle-1 font-weight-thin\">\n                {{ anuncio.descripcion }}\n              </p>\n            </v-tab-item>\n            <v-tab-item>\n              <v-simple-table class=\"pt-5\">\n                <template v-slot:default>\n                  <thead></thead>\n                  <tbody>\n                    <tr>\n                      <td>Estado:</td>\n                      <td v-show=\"anuncio.nuevo === false\">Usado</td>\n                      <td v-show=\"anuncio.nuevo === true\">Nuevo</td>\n                    </tr>\n                    <tr>\n                      <td>Marca:</td>\n                      <td>{{ anuncio.marca }}</td>\n                    </tr>\n                    <tr>\n                      <td>Modelo:</td>\n                      <td>{{ anuncio.modelo }}</td>\n                    </tr>\n                    <tr>\n                      <td>Pantalla:</td>\n                      <td>{{ anuncio.pantalla }} pulgadas</td>\n                    </tr>\n                    <tr>\n                      <td>Sistema:</td>\n                      <td>{{ anuncio.sistema }}</td>\n                    </tr>\n                    <tr>\n                      <td>ROM:</td>\n                      <td>{{ anuncio.rom }} GB</td>\n                    </tr>\n                    <tr>\n                      <td>RAM:</td>\n                      <td>{{ anuncio.ram }} GB</td>\n                    </tr>\n                  </tbody>\n                </template>\n              </v-simple-table>\n            </v-tab-item>\n          </v-tabs>\n        </v-col>\n      </v-row>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport { db } from \"../db\";\nimport { bus } from \"../main\";\nexport default {\n  data() {\n    return {\n      anuncio: {},\n    };\n  },\n\n  methods: {\n    async mostrarProducto() {\n      await db\n        .collection(\"productos\")\n        .doc(this.$route.params.id)\n        .get()\n        .then((r) => {\n          this.anuncio = r.data();\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n    agregarproducto() {\n      bus.$emit(\"addCarrito\", this.anuncio);\n    },\n  },\n\n  created() {\n    this.mostrarProducto();\n  },\n};\n</script>\n"]}]}